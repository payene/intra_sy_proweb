{% extends 'base.html.twig' %}

{% block body %}
    <h1>AffectationLogin</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ affectationLogin.id }}</td>
            </tr>
            <tr>
                <th>Debutaffectation</th>
                <td>{% if affectationLogin.debutAffectation %}{{ affectationLogin.debutAffectation|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Finaffectation</th>
                <td>{% if affectationLogin.finAffectation %}{{ affectationLogin.finAffectation|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('affectationlogin_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('affectationlogin_edit', { 'id': affectationLogin.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
